{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "server",
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false
  },
  "include": ["src/opensky/**/*"]
}
